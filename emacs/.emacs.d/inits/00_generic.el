;;; undo limitation
(setq undo-limit 1000000)
(setq undo-strong-limit 1300000)

;;; Do not create back up file
;;; http://www.cozmixng.org/~kou/emacs/dot_emacs
(setq backup-inhibited t)

;;; Always insert final new line
;;; http://www.cozmixng.org/~kou/emacs/dot_emacs
(setq require-final-newline t)

